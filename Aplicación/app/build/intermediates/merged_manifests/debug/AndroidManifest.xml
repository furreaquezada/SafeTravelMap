            android:exported="false" />
        <activity
            android:name="com.example.safetravelmap.LoginActivity"
            android:exported="false" />
        <activity
            android:name="com.example.safetravelmap.MainActivity"
            android:exported="true" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
